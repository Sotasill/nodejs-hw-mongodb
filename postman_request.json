{
  "info": {
    "name": "Create Contact with Photo",
    "method": "POST",
    "url": "http://localhost:3000/contacts",
    "description": "Создание нового контакта с фотографией"
  },
  "header": [
    {
      "key": "Authorization",
      "value": "Bearer ваш_токен_доступа",
      "type": "text"
    }
  ],
  "body": {
    "mode": "formdata",
    "formdata": [
      {
        "key": "photo",
        "type": "file",
        "src": "выберите_файл"
      },
      {
        "key": "name",
        "value": "Иван Петров",
        "type": "text"
      },
      {
        "key": "phoneNumber",
        "value": "+380991234567",
        "type": "text"
      },
      {
        "key": "email",
        "value": "ivan@example.com",
        "type": "text"
      }
    ]
  }
}
